{
  "expo": {
    "name": "WeatherShowcase",
    "slug": "WeatherShowcase",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sun.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/mist.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "package": "com.kwyw.weather"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cloud.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kwyw.weather"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/fullStar.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
